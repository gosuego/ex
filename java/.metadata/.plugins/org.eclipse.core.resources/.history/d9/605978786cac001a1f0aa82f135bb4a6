import java.util.Scanner;
public class SeasonExSwitch {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("달을 입력하세요(1~12)>>");
		int month = scanner.nextInt();
		switch(month) {
		case 1:
		case 2:
		case 12:
			month = '겨울';
			break;
		case 3:
		case 4:
		case 5:
			month = '봄';
			break;
		case 6:
		case 7:
		case 8;
		month = '여름';
		break;
		case 9;
		case 10;
		case 11;
		month = '가을';
		}

	}

}
